
.popup-content {
  padding: 20rpx 0;
  background-color: #fff;
  border-radius: 16rpx 16rpx 0 0;
}
.popup-item {
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-bottom: 1rpx solid #f5f5f5;
}
.popup-text {
  font-size: 32rpx;
  color: #333;
}
.popup-item.cancel {
  margin-top: 16rpx;
}
.cancel .popup-text {
  color: #666;
}
